<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <record id="module_ss_msd" model="ir.module.category">
            <field name="name">MsdSsm</field>
            <field name="description">Helps you manage your users.</field>
            <field name="sequence">15</field>
        </record>
        <record id="group_ssm_user_l1" model="res.groups">
            <field name="name">SSM_User_L1</field>
            <field name="category_id" ref="module_ss_msd"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="group_ssm_user_l2" model="res.groups">
            <field name="name">SSM_User_L2</field>
            <field name="category_id" ref="module_ss_msd"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="implied_ids" eval="[(4, ref('base.group_user'))]"/>
        </record>
        <record id="group_ssm_other" model="res.groups">
            <field name="name">SSM_Other</field>
            <field name="category_id" ref="module_ss_msd"/>
            <field name="users" eval="[(4, ref('base.user_root'))]"/>
            <field name="implied_ids" eval="[(4, ref('msd_ssm.group_ssm_user_l1')), (4, ref('base.group_user'))]"/>
         </record>
    </data>
</odoo>
